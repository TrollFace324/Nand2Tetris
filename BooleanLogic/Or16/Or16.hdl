/**
 * 16-bit bitwise Or:
 * for i = 0..15 out[i] = (a[i] or b[i])
 */

CHIP Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    Nand(a=a[i], b=a[i], out=notAndA{i});
    Nand(a=b[i], b=b[i], out=notAndB{i});
    Nand(a=notAndA{i}, b=notAndB{i}, out=out[i]);
}