/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a=sel, b=b, out=N);
    Nand(a=sel, b=sel, out=C);
    Nand(a=C, b=a, out=D);
    Nand(a=D, b=N, out=out);
}