/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=N);
    Nand(a=a, b=N, out=C1);
    Nand(a=b, b=N, out=C2);
    Nand(a=C1, b=C2, out=out);
}